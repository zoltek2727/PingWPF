<Window x:Class="PingWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PingWPF"
        mc:Ignorable="d"
        Title="Ping test" Height="400" Width="500">
    <Grid>
        <TextBox Name="TextBoxAdres" HorizontalAlignment="Left" Height="30" Margin="10,300,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="220" RenderTransformOrigin="0.332,0.494" FontWeight="Bold" Background="White" Foreground="#FF534D4D" />
        <Button Name="ButtonWyslij" Margin="262,300,10,0" VerticalAlignment="Top" Height="30" Click="ButtonWyslij_Click"/>
        <Label Content="Adres do testowania ping" HorizontalAlignment="Left" Margin="10,274,0,0" VerticalAlignment="Top" Width="220"/>
        <ProgressBar Name="ProgressBarCzas" Height="30" Margin="10,239,10,0" VerticalAlignment="Top"/>
        <Label Content="Czas który upłynął od rozpoczęcia testu PING." Margin="10,208,10,0" VerticalAlignment="Top" RenderTransformOrigin="-0.237,0.846"/>
        <ListView Name="ListViewLista" Margin="10,10,10,0" Background="Black" Foreground="#FF03E8FF" Height="198" VerticalAlignment="Top"/>
    </Grid>
</Window>
